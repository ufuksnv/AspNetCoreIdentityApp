@using AspNetCoreIdentityApp.Areas.Admin.Models
@model List<UserViewModel>
@{
    ViewData["Title"] = "UserList";
}

<h1>UserList</h1>
<br />
<table class="table table-striped">
    <thead>
        <tr>
         <th>Id</th>
         <th>User Name</th>
         <th>Email</th>
        </tr>
    </thead>

    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.UserId</td>
                <td>@item.UserName</td>
                <td>@item.UserEmail</td>
            </tr>
           
        }
    </tbody>

</table>
